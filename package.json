{
    "name": "als-oracle-pathfinder",
    "description": "An Account Lookup Service Oracle using GSMA PathFinder for resolution of MSISDNs",
    "version": "11.0.2",
    "author": "Matt Kingston <matt.kingston@modusbox.com>",
    "contributors": [],
    "repository": {
        "type": "git",
        "url": "git://github.com/mojaloop/als-oracle-pathfinder.git"
    },
    "bugs": "http://github.com/mojaloop/als-oracle-pathfinder/issues",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.3",
        "@hapi/hapi": "20.1.5",
        "@lib/pathfinder": "file:src/lib/pathfinder",
        "@mojaloop/central-services-logger": "10.6.1",
        "@mojaloop/central-services-shared": "^14.0.0",
        "dotenv": "^10.0.0",
        "hapi-openapi": "^3.0.0",
        "knex": "^0.95.10",
        "mysql2": "^2.3.0"
    },
    "devDependencies": {
        "ava": "^3.15.0",
        "eslint": "7.32.0",
        "npm-audit-resolver": "2.3.1",
        "npm-check-updates": "11.8.3",
        "nyc": "^15.1.0",
        "sinon": "^11.1.2",
        "standard-version": "^9.3.2"
    },
    "scripts": {
        "build": "docker build -t als-oracle-pathfinder -f ./Dockerfile .",
        "lint": "eslint .",
        "regenerate": "yo swaggerize:test --framework hapi --apiPath './src/swagger.json'",
        "start": "node --unhandled-rejections=strict ./src/index",
        "test": "PATHFINDER_REJECT_UNAUTHORIZED=true ava ./test/unit/**/*.test.js",
        "test:pathfinder": "ava ./src/lib/pathfinder/test/unit/**/*.test.js",
        "test:coverage": "nyc ava -- 'test/unit/**/*.test.js' | nyc report ---reporter=text",
        "test:coverage-check": "npm run test:coverage && nyc check-coverage",
        "audit:resolve": "SHELL=sh resolve-audit --production",
        "audit:check": "SHELL=sh check-audit --production",
        "dep:check": "npx ncu -e 2",
        "dep:update": "npx ncu -u",
        "release": "standard-version --releaseCommitMessageFormat 'chore(release): {{currentTag}} [skip ci]'"
    },
    "generator-swaggerize": {
        "version": "4.1.0"
    }
}
